n1 = int(input("Enter how many terms you want to get of Fibonacci Series"))

a = 0
b = 1
count = 0

if n1 <= 0:
    print("Please enter any positive integer")
else:
    print("Fibonacci series is :")
    while count<n1:
        print(a)
        c = a + b
        a = b
        b = c
        count +=1
